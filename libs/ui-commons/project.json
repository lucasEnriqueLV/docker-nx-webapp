{
  "projectType": "library",
  "root": "libs/ui-commons",
  "sourceRoot": "libs/ui-commons/src",
  "prefix": "projects",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui-commons"],
      "options": {
        "jestConfig": "libs/ui-commons/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui-commons/src/**/*.ts",
          "libs/ui-commons/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
